{
  "name": "ferp",
  "module": "src/ferp.js",
  "main": "ferp.js",
  "description": "Build functional and pure applications in NodeJS and the Browser!",
  "version": "1.1.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrozbarry/ferp.git"
  },
  "keywords": [
    "function",
    "reactive",
    "programming",
    "frp",
    "framework",
    "elm-like",
    "application"
  ],
  "author": "Alex Barry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrozbarry/ferp/issues"
  },
  "homepage": "https://github.com/mrozbarry/ferp#readme",
  "scripts": {
    "test": "nyc ava",
    "lint": "eslint ./src",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "push": "yarn test && yarn publish"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^2.0.0",
    "@ava/babel-preset-transform-test-files": "^4.0.1",
    "@babel/register": "^7.0.0",
    "ava": "1.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nyc": "^13.3.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel-minify": "^6.0.0",
    "rollup-plugin-eslint": "^5.0.0",
    "sinon": "^7.2.3"
  },
  "ava": {
    "files": [
      "src/**/**/*.test.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "cache": true,
    "failFast": true,
    "verbose": true,
    "compileEnhancements": false,
    "require": [
      "@babel/register"
    ]
  }
}
